<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fayenp.actualfayeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fayeappicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fayeappicon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ActualFAYEApp">
        <activity android:name="com.fayenp.actualfayeapp.DisplayMoodsActivity"
            android:parentActivityName="com.fayenp.actualfayeapp.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.fayenp.actualfayeapp.MainActivity"/>
        </activity>
        <activity
            android:name="com.fayenp.actualfayeapp.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>